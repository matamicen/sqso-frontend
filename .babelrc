{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true,
          "esmodules": true

        }
      
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
  
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
